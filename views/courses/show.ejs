<h2 class="center"> Course Data for <%= course.name%></h2>
<table class="table">
    <tbody>
        <tr>
            <td>Name:</td>
            <td>
                <%= course.title %>
            </td>
        </tr>
        <tr>
            <td>Description:</td>
            <td>
                <%= course.description %>
            </td>
        </tr>
        <tr>
            <td>zip Code:</td>
            <td>
                <%= course.zipCode %>
            </td>
        </tr>
        <tr>
            <td>Cost:</td>
            <td>
                <%= course.cost %>
            </td>
        </tr>
        <tr>
            <td>Maximum Students:</td>
            <td>
                <%= course.maximumStudents %>
            </td>
        </tr>
    </tbody>
</table>